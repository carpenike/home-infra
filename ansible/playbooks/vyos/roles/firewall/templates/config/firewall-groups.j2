{%- if config['group'] is undefined -%}
  {%- set _ = config.update({'group': dict()}) -%}
{%- endif -%}

{%- for key, values in firewall_groups.items() -%}
  {%- set _ = config['group'].update({key: values}) -%}
{%- endfor -%}

